<%= render 'admin/tickets/ticket', ticket: @ticket %>
<div>
  <%= link_to "Back", admin_tickets_path, class: "btn btn-xs btn-primary" %>
  <%= link_to "Edit", edit_admin_ticket_path(@ticket), class: "btn btn-xs btn-success" %>
  <%= button_to "Delete", admin_ticket_path(@ticket), method: :delete, class: "btn btn-xs btn-danger" %>
</div>
